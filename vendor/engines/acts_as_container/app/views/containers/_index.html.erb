<%#= display_objects_list(
  #:collection => @paginated_objects,
  :in_list_partial => "admin/#{params[:controller].split('/')[1]}/container_in_list",
  :ajax_url => send("admin_#{params[:controller].split('/')[1]}_path"),
  #:ajax_url => request.path+'?'+request.url.split('?').last,
  :ordering_fields => ['created_at', 'title'],
  :output_formats => ['xml', 'json', 'atom'],
  :no_div => @no_div
) %>

Title:
  <%= object.title %>
  <% if object.has_permission_for?('edit', @current_user, current_container_type)  %>
    <%= link_to 'edit', edit_container_path(object)  %>
  <% end %>
  <% if object.has_permission_for?('destroy', @current_user, current_container_type)  %>
    <%= link_to 'delete', container_path(object), :method => :delete, :confirm => 'Are You Sure?'  %>
  <% end %>
<br />
Description:
<p><%= object.description  %></p><br />
