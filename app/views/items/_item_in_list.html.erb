<% div_for item_in_list do %>
  <div class="item-list">
    <%= link_to item_in_list.title, item_in_list, :class => 'object' %> dans <%= link_to "lien workspace", item_in_list %> par <%= link_to_user item_in_list.user %> le samedi 10 septembre
    <div style="float:left"><%= item_in_list.comments.count %> commentaires - Note : </div>
    <div style="float:left" id="rating_<%= item_in_list.class.to_s.underscore %>_<%= item_in_list.id %>" style="width:85px;"></div>
    <div class="clear"></div>
  </div>
	<script type="text/javascript" charset="utf-8">
		new Starbox("rating_<%= item_in_list.class.to_s.underscore %>_<%= item_in_list.id %>", <%= item_in_list.rating %>, { locked: true });	
	</script>	
<% end %>
