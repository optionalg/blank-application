<% item_reactive_content_tag(:div, object) do %>
	<span><%= image_tag object.icon %></span>
	<span style="width: 10em"><%= object.title %></span>
	<span style="width: 10em">
		<%= object.user.firstname.capitalize %>
		<%= object.user.lastname.capitalize %>
	</span>
	<span><%= object.created_at.l(:long) %></span>
<% end %>