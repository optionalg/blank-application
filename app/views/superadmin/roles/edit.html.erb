<%= superadmin_tabs_generator() %>

<div class="objectList">
	<div class="blockCornerLeft"></div><div class="blockElementHeader sevH">
		<span id="item_count">Edit Roles</span>
	</div><div class="blockCornerRight"></div>

	<div class="contentList superadmin">

		<% form_for @role, 
					:url => superadmin_role_path(@role.id), 
					:builder => BlankFormBuilder, 
					:method => :put do |f| %>

  			<%= hidden_field_tag :id, @role.id %>

  			<%= render :partial => 'form', :locals => {:f => f} %>
  			
    		<%= f.submit "Update", :class => "submit_button" %>
		<% end %>
	</div>
</div>