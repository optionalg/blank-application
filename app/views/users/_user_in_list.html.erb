<%= "#{object.login} (#{object.full_name})" %>
<%= link_to 'show', user_path(object.id) %>
<% if @current_user.has_system_role('superadmin') %>
  <%= link_to 'edit', edit_user_path(object.id) %>
  <%= link_to 'delete', user_path(object.id), :method => :delete, :confirm => 'Really ?' %>
<% end %>
