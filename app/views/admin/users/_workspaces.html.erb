<% if ws_roles = object.workspace_roles.uniq and !ws_roles.empty? %>
    <% ws_roles.each do |r| %>
      	<%= I18n.t('general.common_word.role') + ' \'' + I18n.t('general.role.'+r.name) + '\''%>
      	<ul class="wsList">
        <% Workspace.allowed_user_with_ws_role(object, r.name).each do |ws| -%>
        	<li><%= link_to ws.title, admin_workspace_path(ws.id) %></li>
        <% end -%>
      	</ul>
    <% end %>
<% end %>
